            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/game_1_min"
            app:layout_constraintTop_toTopOf="@+id/game_1" />


        <TextView
            android:id="@+id/game_2"
            android:layout_width="@dimen/table_field"
            android:layout_height="wrap_content"
