        <TextView
            android:id="@+id/game_4"
            android:layout_width="@dimen/table_field"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="@dimen/tight_margin"
            android:text="game 4"
            app:layout_constraintEnd_toStartOf="@+id/game_4_min"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/game_3" />

        <TextView
            android:id="@+id/game_4_min"
            android:layout_width="@dimen/table_field"
            android:layout_height="wrap_content"
            android:text="min 4"
            app:layout_constraintEnd_toStartOf="@+id/game_4_max"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/game_4"
