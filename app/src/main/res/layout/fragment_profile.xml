            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/game_2_min"
            app:layout_constraintTop_toTopOf="@+id/game_2" />


        <TextView
            android:id="@+id/game_3"
            android:layout_width="@dimen/table_field"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="@dimen/tight_margin"
            android:text="game 3"
            app:layout_constraintEnd_toStartOf="@+id/game_3_min"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
