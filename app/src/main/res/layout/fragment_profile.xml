            app:layout_constraintTop_toTopOf="@+id/game_0" />

        <TextView
            android:id="@+id/game_1"
            android:layout_width="@dimen/table_field"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="24dp"
            android:text="game 1"
            app:layout_constraintEnd_toStartOf="@+id/game_1_min"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/game_0" />

        <TextView
            android:id="@+id/game_1_min"
            android:layout_width="@dimen/table_field"
            android:layout_height="wrap_content"
            android:text="min 1"
            app:layout_constraintEnd_toStartOf="@+id/game_1_max"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/game_1"
            app:layout_constraintTop_toTopOf="@+id/game_1" />

        <TextView
            android:id="@+id/game_1_max"
